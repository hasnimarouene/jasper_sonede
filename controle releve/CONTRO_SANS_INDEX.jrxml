	<parameter name="DATE_MIN" class="java.lang.String"/>
	<parameter name="DATE_MAX" class="java.lang.String"/>
	<parameter name="ROU_CODE_MAX" class="java.lang.String"/>
	<parameter name="ROU_CODE_MIN" class="java.lang.String"/>
	<queryString>
		<![CDATA[select *
from
(

  select  distinct spt_refe,
           rou_code,
           spt_routeorder,
           equ_realnumber,
           vow_code,
           aindex,
           nindex,
           conso,
           commentaire,
           histo,
           par_shortname,
           adr_shortname
   from
   (
    select spt.spt_refe,
           rou.rou_code,
           spt.spt_routeorder,
           equ.equ_realnumber,
           mdl.vow_code,
           pk_tecmtrread.GetSumIndexById(mrd.mrd_previous_id) aindex,
           '' nindex,
           '' conso,
           pk_tecmtrread.GetCommentaireById(mrd.mrd_id) commentaire,
           replace(pk_tecmtrread.GetHistoReleve(mrd.spt_id, trunc(mrd.mrd_dt),1),'</b>','') histo,
           par.par_shortname,
           adr.adr_shortname
    from  tecroute rou,
           tecservicepoint spt,
           v_genadress adr,
           agrserviceagr sag,
           agrcustomeragr cag,
           techequipment heq,
           tecequipment equ,
           tecequmodel mmo,
           genvocword mdl,
           v_genparty par,
           tecmtrread mrd,
           genvocword vow
    where  rou.rou_id = spt.rou_id
    and    spt.adr_id = adr.adr_id
    and    spt.spt_id = sag.spt_id
    and    trunc(SAG.SAG_STARTDT) = (select trunc(max(sag2.SAG_STARTDT))
                                           from   agrserviceagr sag2
                                           where  sag2.spt_id = SPT.SPT_ID)
    and    sag.cag_id = cag.cag_id
    and    cag.par_id = par.par_id
    and    spt.spt_id = heq.spt_id
    and    trunc(heq.heq_startdt) = (select trunc(max(heq2.heq_startdt))
                                     from   techequipment heq2
                                     where  heq2.spt_id = spt.spt_id)
    and    heq.heq_enddt is null
    and    heq.equ_id = equ.equ_id
    and    equ.mmo_id = mmo.mmo_id
    and    mmo.vow_model = mdl.vow_id
    and    spt.spt_id = mrd.spt_id
    and    mrd.vow_readorig  = vow.vow_id
    and    vow.vow_code in ('TSP','LISTING')
    and    rou.rou_code between $P{ROU_CODE_MIN}
       and    $P{ROU_CODE_MAX}

    and    not exists(select 1
                      from   tecmtrmeasure mme2
                      where  mme2.mrd_id = mrd.mrd_id)
    and    not exists(select 1
                      from   tecmtrread mrd2,
                             genvocword vow2
                      where  mrd2.mrd_previous_id = mrd.mrd_id
                      and    mrd2.vow_readmeth = vow.vow_id
                      and    mrd2.mrd_etatfact = 1
                      and    vow.vow_code = '2')
    union
    select spt.spt_refe,
           rou.rou_code,
           spt.spt_routeorder,
           '' equ_realnumber,
           '' vow_code,
           null aindex,
           null nindex,
           null conso,
           '' commentaire,
           '' histo,
           '' par_shortname,
           adr.adr_shortname
    from   tecroute rou,
           tecservicepoint spt,
           techequipment heq,
           v_genadress adr
    where
      rou.rou_id = spt.rou_id
    and    spt.adr_id = adr.adr_id
    and    spt.spt_id = heq.spt_id
    and    trunc(heq.heq_startdt) = (select trunc(max(heq2.heq_startdt))
                                     from   techequipment heq2
                                     where  heq2.spt_id = spt.spt_id)
    and    heq.heq_enddt is null
    and    not exists(select 1
                      from   tecmtrread mrd2,
                             genvocword vow2
                      where  mrd2.spt_id = spt.spt_id
                      and    mrd2.mrd_etatfact = 1
                      and  not  exists(select 1 from TECREADITEBILL trb where trb.mrd_id =mrd2.mrd_id )
)
and  rou.rou_code between $P{ROU_CODE_MIN} and $P{ROU_CODE_MAX}
   )
  order by rou_code, spt_routeorder

)
]]>
